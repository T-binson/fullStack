<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>ejs</title>
</head>
<body>
	<!-- 
		<%= %>: 等号表示转义输出 
		<%- %>: 减号表示原样输出
	 -->
	<%- include('./components/header.ejs') %>
	<table border="1" width="100%">
		<thead>
			<tr>
				<th>ID</th>
				<th>username</th>
			</tr>
		</thead>
		<tbody>
			<% arr.forEach((json, index) => { %>
			<% if (index === arr.length - 1) { %>
			<tr class="last">
			<%  } else { %>
			<tr>
			<% } %>
				<td><%=json.ID %></td>
				<td><%=json.username %></td>
			</tr>
			<% }) %>
		</tbody>
	</table>
	<%- include('./components/footer.ejs') %>
</body>
</html>